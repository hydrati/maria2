{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "typeRoots": [
      "./src/types"
    ],
    "types": [
      "maria2"
    ],
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true
  }
}